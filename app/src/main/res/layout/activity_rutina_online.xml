<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/naranjasuave"
    tools:context=".RutinaOnlineActivity">

    <ImageView
        android:id="@+id/logo5"
        android:layout_width="239dp"
        android:layout_height="167dp"
        android:layout_marginTop="30dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logogym" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="230dp"
        android:background="@color/black"
        android:text="Rutina Online"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="395dp"
        android:layout_height="430dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="100dp"
        android:layout_marginEnd="10dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/logo5">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="412dp">

            <LinearLayout
                android:layout_width="380dp"
                android:layout_height="353dp"
                android:background="@color/verdeoliva"
                android:orientation="vertical"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView16"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:text="OBJETIVO"
                    android:textAlignment="center"
                    android:textColor="@color/black"
                    android:textSize="30sp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <Button
                    android:id="@+id/volverBtn2"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="@color/verdeclaro"
                    android:onClick="paginaBajarPeso"
                    android:text="Bajar de Peso"
                    android:textColor="@color/white"
                    android:textSize="20dp"
                    tools:layout_constraintTop_toBottomOf="@id/textView8" />

                <Button
                    android:id="@+id/volverBtn11"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="@color/verdeclaro"
                    android:onClick="paginaGanarMasa"
                    android:text="Ganar Masa Muscular"
                    android:textColor="@color/white"
                    android:textSize="20dp"
                    tools:layout_constraintTop_toBottomOf="@id/textView8" />

                <Button
                    android:id="@+id/volverBtn15"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="@color/verdeclaro"
                    android:onClick="paginaDefinicion"
                    android:text="Definicion"
                    android:textColor="@color/white"
                    android:textSize="20dp"
                    tools:layout_constraintTop_toBottomOf="@id/textView8" />

                <Button
                    android:id="@+id/volverBtn16"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="@color/verdeclaro"
                    android:onClick="paginaResistencia"
                    android:text="Resistencia Aerobica"
                    android:textColor="@color/white"
                    android:textSize="20dp"
                    tools:layout_constraintTop_toBottomOf="@id/textView8" />

                <Button
                    android:id="@+id/volverBtn17"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="10dp"
                    android:backgroundTint="@color/verdeclaro"
                    android:onClick="paginaFlexibilidad"
                    android:text="Ganar Flexibilidad"
                    android:textColor="@color/white"
                    android:textSize="20dp"
                    tools:layout_constraintTop_toBottomOf="@id/textView8" />

            </LinearLayout>

            <Button
                android:id="@+id/volverBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="@color/black"
                android:onClick="paginaPrincipal"
                android:text="Volver al menÃº principal"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                tools:layout_constraintTop_toBottomOf="@id/textView8" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>